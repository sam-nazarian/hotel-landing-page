{
  "name": "hotel-landing-page",
  "version": "1.0.0",
  "description": "Hotel Landing Page",
  "browserslist": "last 10 versions",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss css/style.comp.css --use autoprefixer -o css/style.prefix.css",
    "compress:css": "sass css/style.prefix.css css/style.css --style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samanfathnazarian/hotel-landing-page.git"
  },
  "author": "Saman Fathnazarian",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samanfathnazarian/hotel-landing-page/issues"
  },
  "homepage": "https://github.com/samanfathnazarian/hotel-landing-page#readme",
  "devDependencies": {
    "sass": "^1.56.1"
  },
  "dependencies": {
    "autoprefixer": "^10.4.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "postcss-cli": "^10.0.0"
  }
}
